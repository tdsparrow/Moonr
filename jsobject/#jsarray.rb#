module Moonr
  class JSArray 
    def initialize(arr)
      @array = arr
    end

    def size
      @array.size
    end

    alias :length :size
    def to_arr()
      @array
    end
    
    def +(arr)
      JSArray.new(@array + arr.to_arr)
    end

    def <<(entry)
      JSArray.new(@array << entry)
    end
  end
end
